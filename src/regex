s = readtable(Pkg.dir("TimeSeries/test/data/spx.csv"))

# "first find the columns that are UTF8String"

col_to_test = String[]

 for foo in s[1,:]
 typeof(s[1,foo[1]]) == UTF8String?
 push!(col_to_test, foo[1]):
 nothing
 end

# col_to_test
# 1-element Array{String,1}:
#  "Date"

# "then iterate over those columns col in col_to_test"
# "to test if they match the pattern for potential dates"

col_that_pass = String[]

for foo in col_to_test
d = match(r"[-|\s|\/|.]", s[1,foo])
d !== nothing? (bar = split(s[1, foo], d.match)): (bar = [])

if length(bar) == 3
   push!(col_that_pass, foo)
end
end

# "then iterate col in col_that_pass and parse them"

col_to_parse = String[]
for foo in col_that_pass
  s[col_that_pass] = Date[date(d) for d in s[foo]]
end
